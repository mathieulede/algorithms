    "g = gv.Graph()\n",
    "\n",
    "# g.node(\"Hamburgers\")\n",
    "# g.node(\"Milkshakes\")\n",
    "# g.node(\"French Fries\")\n",
    "\n",
    "g.edge(\"Hamburgers\", \"Milkshakes\")\n",
